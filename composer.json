{
  "name": "leonidas/leonidas",
  "description": "WordPress theme and plugin framework.",
  "type": "wordpress-plugin",
  "license": "GPL-3.0-or-later",
  "authors": [
    {
      "name": "Chris Williams",
      "email": "spider.mane.web@gmail.com"
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "conflict": {
    "composer/composer": "<2.0"
  },
  "require": {
    "php": "^8.2",
    "guzzlehttp/psr7": "^2.1",
    "jawira/case-converter": "^3.4",
    "koriym/attributes": "^1.0",
    "league/container": "4.2.0",
    "nesbot/carbon": "^2.54",
    "nette/php-generator": "^4.0",
    "panamax/panamax": "dev-master",
    "performing/twig-components": "^0.6.0",
    "php-parallel-lint/php-console-highlighter": "^1.0",
    "phpdocumentor/reflection-docblock": "^5.3",
    "psr/cache": "^1.0 || ^2.0 || ^3.0",
    "psr/container": "^1.0 || ^2.0",
    "psr/event-dispatcher": "^1.0",
    "psr/http-client": "^1.0",
    "psr/http-factory": "^1.0",
    "psr/http-message": "^1.0 || ^2.0",
    "psr/http-server-handler": "^1.0",
    "psr/http-server-middleware": "^1.0",
    "psr/link": "^1.0 || ^2.0",
    "psr/simple-cache": "^1.0 || ^2.0 || ^3.0",
    "symfony/console": "^5.4 || ^6.0",
    "symfony/filesystem": "^5.4 || ^6.0",
    "symfony/polyfill-php83": "^1.31",
    "symfony/polyfill-php84": "^1.31",
    "symfony/yaml": "^5.4",
    "twig/twig": "^2.4 || ^3.0",
    "webtheory/collection": "dev-master",
    "webtheory/config": "dev-master",
    "webtheory/facade": "dev-master",
    "webtheory/html": "dev-master",
    "webtheory/http-policy": "dev-master",
    "webtheory/saveyour": "dev-master",
    "webtheory/wpdmc": "dev-master"
  },
  "require-dev": {
    "10up/wp_mock": "1.0",
    "brain/monkey": "^2.6",
    "captainhook/captainhook": "^5.10",
    "commerceguys/addressing": "^1.0",
    "composer/installers": "^1.0 || ^2.0",
    "egulias/email-validator": "^2.1",
    "fakerphp/faker": "^1.23",
    "friendsofphp/php-cs-fixer": "^3.3",
    "geocoder-php/google-maps-provider": "^4.4",
    "giggsey/libphonenumber-for-php": "^8.10",
    "google/recaptcha": "^1.2",
    "joshtronic/php-loremipsum": "^2.1",
    "koodimonni/composer-dropin-installer": "^1.3",
    "league/flysystem": "^3.16",
    "mockery/mockery": "^1.4",
    "nikic/php-parser": "^4.13",
    "nunomaduro/termwind": "^1.15",
    "opis/closure": "^3.6",
    "oscarotero/env": "^2.1",
    "php-http/guzzle7-adapter": "^1.0",
    "phpbench/phpbench": "^1.2",
    "phpcompatibility/phpcompatibility-wp": "^2.1",
    "phpmailer/phpmailer": "^6.6",
    "phpoption/phpoption": "^1.8",
    "phpspec/prophecy-phpunit": "^2.0",
    "phpstan/extension-installer": "^1.1",
    "phpstan/phpstan": "^1.6",
    "phpunit/phpunit": "^9.5",
    "phpunit/phpunit-selenium": "^9.0",
    "psy/psysh": "^0.11",
    "rector/rector": "^1.0",
    "respect/validation": "^2.2",
    "roave/security-advisories": "dev-latest",
    "roots/bedrock-autoloader": "^1.0",
    "slim/psr7": "^1.5",
    "slim/slim": "^4.7",
    "smknstd/fakerphp-picsum-images": "^1.0",
    "squizlabs/php_codesniffer": "^3.5",
    "suin/phpcs-psr4-sniff": "^3.0",
    "symfony/asset": "^5.4 || ^6.0",
    "symfony/error-handler": "^5.4 || ^6.0",
    "symfony/finder": "^5.4 || ^6.0",
    "symfony/http-foundation": "^5.4 || ^6.0",
    "symfony/http-kernel": "^5.4 || ^6.0",
    "symfony/mailer": "^5.4 || ^6.0",
    "symfony/messenger": "^5.4 || ^6.0",
    "symfony/string": "^5.4 || ^6.0",
    "symfony/validator": "^5.4 || ^6.0",
    "symfony/var-dumper": "^5.4 || ^6.0",
    "symfony/var-exporter": "^5.4 || ^6.0",
    "szepeviktor/phpstan-wordpress": "^1.1",
    "theseer/tokenizer": "^1.2",
    "twig/cache-extra": "^3.4",
    "twig/inky-extra": "^3.4",
    "vlucas/phpdotenv": "^5.4",
    "webtheory/context": "dev-master",
    "webtheory/exterminate": "dev-master",
    "webtheory/path-builder": "dev-master",
    "webtheory/playground": "dev-master",
    "webtheory/symfony-mailer-pack": "dev-master",
    "webtheory/unit-utils": "dev-master",
    "webtheory/wp-cli-bundle": "dev-master",
    "webtheory/wp-cli-util": "dev-master",
    "webtheory/wp-link": "dev-master",
    "webtheory/wpunit": "dev-master",
    "wpackagist-plugin/classic-editor": "@stable",
    "wpackagist-plugin/debug-bar": "@stable",
    "wpackagist-plugin/disable-gutenberg": "@stable",
    "wpackagist-plugin/fakerpress": "@stable",
    "wpackagist-plugin/wp-native-php-sessions": "@stable",
    "wpackagist-theme/twentytwentythree": "@stable"
  },
  "suggest": {
    "joshtronic/php-loremipsum": "generate lorem placeholder text"
  },
  "autoload": {
    "psr-4": {
      "Leonidas\\": "src/",
      "Example\\App\\": "examples/app/app/",
      "Example\\Plugin\\": "examples/plugin/app/",
      "Example\\Content\\": "examples/content/app/",
      "Example\\Theme\\": "examples/theme/app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "analyze": "vendor/bin/phpstan analyze",
    "asset": "npm --prefix assets",
    "bench": "vendor/bin/phpbench",
    "check": "vendor/bin/phpcs",
    "collect": "vendor/bin/var-dump-server",
    "export": "git archive --output=./build/leonidas.zip --format=zip HEAD",
    "hooks": "vendor/bin/captainhook",
    "hoplite": "bin/hoplite",
    "inspect": "vendor/bin/psalm",
    "link": "vendor/bin/wp-link leonidas && php bin/link-examples",
    "play": "php @playground/console.php",
    "refactor": "vendor/bin/rector process",
    "refactor:d": "@refactor --dry-run",
    "shell": "@wp shell",
    "test": "vendor/bin/phpunit",
    "test:acceptance": "@test --testsuite acceptance",
    "test:functional": "@test --testsuite functional",
    "test:integration": "@test --testsuite integration",
    "test:unit": "@test --testsuite unit",
    "test:wpunit": "@test --testsuite wpunit",
    "watch": "vendor/bin/phpunit-watcher watch",
    "wp": "vendor/bin/wp",
    "build": [
      "@check",
      "@analyze"
    ],
    "style": [
      "vendor/bin/php-cs-fixer fix --verbose",
      "vendor/bin/phpcbf"
    ],
    "setup": [
      "composer install",
      "cp -n .env.example .env",
      "vendor/bin/build-paths",
      "vendor/bin/make-playground",
      "@wp:init"
    ],
    "test:all": [
      "@test:unit",
      "@test:wpunit",
      "@test:integration",
      "@test:functional",
      "@test:acceptance"
    ],
    "wp:init": [
      "@wp core download",
      "@link"
    ],
    "wp:refresh": [
      "@wp db export",
      "@wp db clean --yes",
      "@wp core install"
    ]
  },
  "config": {
    "process-timeout": 0,
    "optimize-autoloader": false,
    "apcu-autoloader": false,
    "sort-packages": true,
    "allow-plugins": {
      "composer/installers": true,
      "composer/package-versions-deprecated": true,
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "koodimonni/composer-dropin-installer": true,
      "php-http/discovery": true,
      "phpstan/extension-installer": true
    }
  },
  "extra": {
    "installer-paths": {
      "wordpress/wp-content/plugins/{$name}": [
        "type:wordpress-plugin"
      ],
      "wordpress/wp-content/mu-plugins/{$name}/": [
        "type:wordpress-muplugin"
      ],
      "wordpress/wp-content/themes/{$name}/": [
        "type:wordpress-theme"
      ],
      "vendor/{$vendor}/{$name}/": [
        "type:wordpress-dropin"
      ]
    },
    "dropin-paths": {
      "wordpress/wp-content/": [],
      "wordpress/wp-content/mu-plugins": []
    },
    "build-paths": {
      "build": {
        "php-cs-fixer": {},
        "phpcs": {},
        "phpstan": {},
        "phpunit": {},
        "psalm": {},
        "wordpress": {}
      },
      "var": {
        "cache": {},
        "logs": {},
        "temp": {}
      }
    },
    "playground": {
      "contexts": [
        "console",
        "browser",
        "runtime",
        "init",
        "loaded"
      ],
      "bootstrap": "tests/bootstrap.php",
      "web-root": "wordpress"
    }
  }
}
